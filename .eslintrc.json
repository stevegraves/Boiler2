{
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "flowtype",
        "compat"
    ],
    "rules": {
        "compat/compat": 2
    },
    "env": {
        "browser": true,
        "jest": true
    },
    "settings": {
        "polyfills": ["fetch"]
    }
}